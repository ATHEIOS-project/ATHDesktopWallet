{
  "name": "Ether1Wallet",
  "version": "0.1.2",
  "description": "Desktop wallet for Ether1 currency",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "Ether1DesktopWallet",
    "win": {
      "icon": "build/icon.png",
      "target": "7z"
    }
  },
  "repository": "https://github.com/taeguscromis/Ether1DesktopWallet",
  "keywords": [
    "Ether1",
    "Desktop",
    "Wallet"
  ],
  "author": "Ether1",
  "license": "CC0-1.0",
  "dependencies": {
    "adm-zip": "^0.4.13",
    "electron-storage": "^1.0.7",
    "handlebars": "^4.0.12",
    "nedb": "^1.8.0",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "electron": "^3.0.12",
    "electron-builder": "^20.38.3"
  }
}
